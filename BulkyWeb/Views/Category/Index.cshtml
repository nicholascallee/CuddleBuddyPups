@model List<Category>

<h1>Category List</h1>

<table class="table table-bordered table-striped">
    <thead>
    <tr>
        <th>
            cat1
        </th>
        <th>
            cat2
        </th>
    </tr>
    </thead>
    <tbody>
        @foreach(var obj in Model.OrderBy(u=>u.DisplayOrder)){
        <tr>
            <td>@obj.Name</td>
            <td>
                @obj.DisplayOrder
            </td>
        </tr>
        }
    </tbody>


</table>